{
    "name": "exeque/fluent-assert",
    "description": "Fluent interface for Webmozart Assert",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "pestphp/pest": "^3.8|^4.0",
        "symfony/console": "^7.2",
        "symfony/var-dumper": "^7.2",
        "laravel/pint": "^1.21",
        "exeque/php-dedent": "^0.2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ExeQue\\FluentAssert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Morten Harders",
            "email": "mmh@harders-it.dk"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "keywords": [
        "assert",
        "fluent",
        "webmozart",
        "php",
        "assertion",
        "validation"
    ],
    "scripts": {
        "test": "vendor/bin/pest -p",
        "lint": "vendor/bin/pint -p",
        "build": [
            "@php bin/console generate:mixin",
            "@php bin/console generate:docblock-mixin",
            "@composer lint"
        ],
        "pre-commit": [
            "@composer build",
            "@composer test",
            "@composer lint"
        ]
    }
}
